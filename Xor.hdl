/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a,b=b,out=Nandab);
    Nand(a=a,b=Nandab,out=aNandab);
    Nand(a=Nandab,b=b,out=Nandabb);
    Nand(a=aNandab,b=Nandabb,out=out);
}